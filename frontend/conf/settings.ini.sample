# Common
[common]
timezone = Europe/Paris
units = C

# OpenWeatherMap
[owm]
owm_id = <YOUR OWM KEY>
owm_city_id = <YOUR CITY ID>

# Nest
[nest]
nest_username = <MAIL ADDRESS>
nest_password = <PASSWORD>
nest_sn = <NEST SERIAL NUMBER>
nest_index = 0
# Set to true if you have Nest Protect product
nest_protect = false

# Database
[mysql]
mysql_username = <MYSQL USERNAME>
mysql_password = <MYSQL PASSWORD>
mysql_hostname = <MYSQL IP>
mysql_database = <MYSQL DATABASE>


